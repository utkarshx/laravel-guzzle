{
    "name": "utkarshx/laravel-guzzle-provider",
    "description": "Guzzle Service Provider for Laravel 6",
    "keywords": ["laravel", "guzzle", "http"],
    "license": "MIT",
    "authors": [
        {
            "name": "Utkarsh Shukla",
            "email": "utk.shukla@gmail.com"
        }
